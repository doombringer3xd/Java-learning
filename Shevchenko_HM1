package com.company.Homework.First;

public class Two {
    public static void main(String[] args) {
        int a = 25 ;
        int b = 15 ;
        int k ;
        System.out.println(a + b);
        System.out.println(a - b);
        System.out.println(a * b);
        System.out.println(a / b);
        System.out.println(a % b);
    }
}
